{
    "arrowParens": "always",
    "bracketSpacing": true,
    "embeddedLanguageFormatting": "auto",
    "endOfLine": "lf",
    "htmlWhitespaceSensitivity": "css",
    "insertPragma": false,
    "jsxBracketSameLine": false,
    "jsxSingleQuote": false,
    "overrides": [
        {
            "files": "*.{yaml,yml}",
            "options": {
                "tabWidth": 2
            }
        },
        {
            "files": "*.{md,markdown}",
            "options": {
                "tabWidth": 2
            }
        }
    ],
    "plugins": [],
    "pluginSearchDirs": [],
    "printWidth": 80,
    "proseWrap": "always",
    "quoteProps": "as-needed",
    "requirePragma": false,
    "semi": true,
    "singleQuote": false,
    "tabWidth": 4,
    "trailingComma": "none",
    "useTabs": false
}
